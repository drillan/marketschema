{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "VolumeInfo",
  "description": "出来高と売買代金を表現する",
  "type": "object",
  "properties": {
    "symbol": {
      "$ref": "definitions.json#/$defs/Symbol",
      "description": "銘柄識別子"
    },
    "timestamp": {
      "$ref": "definitions.json#/$defs/Timestamp",
      "description": "データ取得時刻"
    },
    "volume": {
      "$ref": "definitions.json#/$defs/Size",
      "description": "出来高（数量ベース）"
    },
    "quote_volume": {
      "oneOf": [
        {
          "$ref": "definitions.json#/$defs/Size"
        },
        {
          "type": "null"
        }
      ],
      "description": "売買代金（決済通貨建て）"
    },
    "open_interest": {
      "oneOf": [
        {
          "type": "number",
          "minimum": 0
        },
        {
          "type": "null"
        }
      ],
      "description": "建玉残（未決済契約数）"
    }
  },
  "required": [
    "symbol",
    "timestamp",
    "volume"
  ],
  "unevaluatedProperties": false
}

{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "OHLCV",
  "description": "ローソク足データを表現する（始値、高値、安値、終値、出来高）",
  "type": "object",
  "properties": {
    "symbol": {
      "$ref": "definitions.json#/$defs/Symbol",
      "description": "銘柄識別子"
    },
    "timestamp": {
      "$ref": "definitions.json#/$defs/Timestamp",
      "description": "足の開始時刻"
    },
    "open": {
      "oneOf": [
        {
          "$ref": "definitions.json#/$defs/Price"
        },
        {
          "type": "null"
        }
      ],
      "description": "始値"
    },
    "high": {
      "oneOf": [
        {
          "$ref": "definitions.json#/$defs/Price"
        },
        {
          "type": "null"
        }
      ],
      "description": "高値"
    },
    "low": {
      "oneOf": [
        {
          "$ref": "definitions.json#/$defs/Price"
        },
        {
          "type": "null"
        }
      ],
      "description": "安値"
    },
    "close": {
      "oneOf": [
        {
          "$ref": "definitions.json#/$defs/Price"
        },
        {
          "type": "null"
        }
      ],
      "description": "終値"
    },
    "volume": {
      "oneOf": [
        {
          "$ref": "definitions.json#/$defs/Size"
        },
        {
          "type": "null"
        }
      ],
      "description": "出来高"
    },
    "quote_volume": {
      "oneOf": [
        {
          "$ref": "definitions.json#/$defs/Size"
        },
        {
          "type": "null"
        }
      ],
      "description": "売買代金（決済通貨建ての出来高）"
    }
  },
  "required": [
    "symbol",
    "timestamp"
  ],
  "unevaluatedProperties": false
}

{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://marketschema.example.com/schemas/trade",
  "title": "Trade",
  "description": "個別約定（歩み値 / Time & Sales）を表現する",
  "type": "object",
  "properties": {
    "symbol": {
      "$ref": "definitions.json#/$defs/Symbol",
      "description": "銘柄識別子"
    },
    "timestamp": {
      "$ref": "definitions.json#/$defs/Timestamp",
      "description": "約定時刻"
    },
    "price": {
      "$ref": "definitions.json#/$defs/Price",
      "description": "約定価格"
    },
    "size": {
      "$ref": "definitions.json#/$defs/Size",
      "description": "約定数量"
    },
    "side": {
      "$ref": "definitions.json#/$defs/Side",
      "description": "売買方向（buy/sell）"
    }
  },
  "required": ["symbol", "timestamp", "price", "size", "side"],
  "unevaluatedProperties": false
}
